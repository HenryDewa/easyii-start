{
    "name": "noumo/easyii-start",
    "description": "Eeasy CMS based on Yii2 Framework",
    "keywords": ["yii2", "cms", "control panel", "admin", "easy"],
    "homepage": "http://easyiicms.com",
    "type": "cms",
    "license": "BSD-3-Clause",
    "support": {
        "wiki": "http://easyiicms.com/docs",
        "source": "https://github.com/noumo/easyii.git",
        "issues": "https://github.com/noumo/easyii/issues"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "noumo/easyii": "*",
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "assets": "0777",
                    "uploads": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}